###################################################################
# this document contains all important variables for the analysis #
###################################################################



# DROPS
droplimit = 0.2             # threshold when something should be considered as drop
dropdiff = 2               # when a drop is considered as a new drop
dropboundary = 2000         # how many values before and after the drop should be visualized
nodrop = 1                  # threshold in ms to exclude fale-positive drop. 0 for off

